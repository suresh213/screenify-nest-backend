  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node -r esm dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pm2TestRun": "NODE_ENV=test PORT=8012 pm2 start dist/main.js",
    "pm2TestRestartRun": "NODE_ENV=test PORT=8012 pm2 restart dist/main.js",
    "pm2ProductionRun": "NODE_ENV=production PORT=8001 pm2 start dist/main.js",
    "pm2ProductionRestartRun": "NODE_ENV=production PORT=8001 pm2 restart dist/main.js"
  },
